# ASDF
. $HOME/.asdf/asdf.sh
. $HOME/.asdf/completions/asdf.bash

# Include pkg
path+=("$HOME/pkg")
path+=("$HOME/pkg/bin")
export PATH

# Include scripts
path+=("$HOME/scripts")
export PATH

# Auto-completion
source <(kubectl completion zsh)
source '/home/erik/pkg/azure-cli/az.completion'

echo " > .zshrc_sources loaded"
